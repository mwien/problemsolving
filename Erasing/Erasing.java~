import java.util.Scanner;


public class Erase {
    
    public static void process(int[][] M, int[] A, int N) {
        for(int i = 0; i < N; i++) {
            M[i][0] = i;
        }
        
        for(int j = 1; 1 << j <= N; j++) {
            for(int i = 0; i + (1 << j) - 1 < N; i++) {
                if(A[M[i][j-1]] >= A[M[i+(1 << (j-1))][j-1]]) {
                    M[i][j] = M[i][j-1];
                } else {
                    M[i][j] = M[i + (1 << (j-1))][j-1];
                }
            }
        }
    }
    
    public static int query(int[][] M, int[] A, int N, int i, int j) {
        int k = (int) (Math.log(j - i + 1)/Math.log(2));
        if(A[M[i][k]] <= A[M[j- (1 << k) + 1][k]]) {
            return M[i][k];
        } else {
            return M[j - (1 << k) + 1][k];
        }
    }

    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        while(true) {
            int N = s.nextInt();
            int D = s.nextInt();
            if(N == 0 && D == 0) {
                break;
            }
            s.nextLine();
            String in = s.nextLine();
            int[] nums = new int[in.length()];
            boolean[] del = new boolean[in.length()];
            for(int i = 0; i < in.length(); i++) {
                nums[i] = Integer.parseInt(Character.toString(in.charAt(i)));
            }
            int[][] M = new int[N][(int) (Math.log(N)/Math.log(2)) + 1];
            process(M, nums, N);
            int startind = 0;
            while(D > 0) {
                int max = query(M, nums, N, startind, startind+D);
                System.out.print(nums[max]);
                D -= max - startind;
                startind = max+1;
            }
            for(int i = startind; i < N && N - i > D; i++) {
                System.out.print(nums[i]);
            }
            System.out.println();
        }
    }

}
