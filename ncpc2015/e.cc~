#include<iostream>
#include<algorithm>
#include<list>

using namespace std;

pair<int, int> j[100005];

int main()
{
    int n, k;
    cin >> n >> k;
    for(int i = 0; i < n; ++i) {
	int a, b;
	cin >> a >> b;
	j[i] = {b, a};
    }
    sort(j, j+n);
    list<pair<int, int>> m;
    for(int i = 0; i < k; ++i) {
	m.push_back({i, -1});
    }
    
}
