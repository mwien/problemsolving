#include<iostream>

using namespace std;

int main()
{
    int N, L, S, T;    
    while(cin >> N >> L >> S >> T) {
	const int nos = N;
	int g[nos][nos] = {0};
	int s[nos][nos] = {0};
	s[S-1][S-1] = 1;
	for(int i = 0; i < N; i++) {
	    int way;
	    for(int j = 0; j < 4; j++) {
		cin >> way;
		g[i][way-1]++;
	    }
	}
	//calculating s * g^L
	//start with g^L by succ squaring
	
    }
}
