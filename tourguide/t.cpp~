#include<iostream>
#include<complex>

using namespace std;

complex<double> pos[10];
complex<double> mv[10];

int main()
{
    int T;
    cin >> T;
    for(int t = 0; t < T; ++t) {
	int n;
	cin >> n;
	double v;
	cin >> v;
	for(int i = 0; i < n; ++i) {
	    double x, y, a, b;
	    cin >> x >> y >> a >> b;
	    pos[i] = complex<double> (x,y);
	    mv[i] = polar(a,b);
	}
    }
}
