#include<iostream>
#include<complex>

using namespace std;

int main()
{
    double ta, tb;
    cin >> ta >> tb;
    complex<double> l(ta, tb);
    cin >> ta >> tb;
    complex<double> d(ta, tb);
    cin >> ta >> tb;
    complex<double> cb(ta, tb);
    double rb;
    cin >> rb;
    cin >> ta >> tb;
    complex<double> cr(ta, tb);
    double rr;
    cin >> rr;
}
