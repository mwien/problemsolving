#include<iostream>
#include<vector>

using namespace std;

int main()
{
    int N;
    cin >> N;
    vector<int> cost;
    for(int i = 0; i < N-1; ++i) {
	int new_cost;
	cin >> new_cost;
	cost.push_back(new_cost);
    }
    vector<int> time;
    for(int i = 0; i < N; ++i) {
	int new_time;
	cin >> new_time;
	if(i == 0) continue;
	time.push_back(new_time);
    }
    
}
