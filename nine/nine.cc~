#include<iostream>
#include<vector>

using namespace std;

int main()
{
    int H;
    cin >> H;
    vector<int> h;
    for(int i = 0; i < H; ++i) {
	int a;
	cin >> a;
	h.push_back(a);
    }
    int B;
    cin >> B;
    vector<int> b;
    for(int i = 0; i < B; ++i) {
	int a;
	cin >> a;
	b.push_back(b);
    }
    
}
