#include<iostream>

using namespace std;

int sl[100005];
bool lt[100005];

int main()
{
    int n, m, k, t;
    cin >> n >> m >> k >> t;
    int mn = 1e9;
    for(int i = 0; i < n; ++i) {
	cin >> sl[i];
	mn = min(mn, sl[i]);
    }
    int hi = 0;
    for(int i = 0; i < m; ++i) {
	int d;
	cin >> d;
	for(int j = t; j >= 0; --j) {
	    if(j-d >= 0) lt[j] |= lt[j-d];
	    if(lt[j]) hi = max(hi,j);
	}
    }
    
}
