#include<iostream>

using namespace std;

string sk(string p, string c) {
    string key = "";
    for(int i = 0; i < p.size(); ++i) {
	key += p[i] - c[i];
    }
}

int main() {
    while(true) {
	int n;
	cin >> n;
	vector<string> plain (n), cipher (n);
	for(int i = 0; i < n; ++i) {
	    cin >> plain[i] >> cipher[i];
	}
	
    }
}
