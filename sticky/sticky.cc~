#include<iostream>
#include<vector>
#include<algorithm>

using namespace std;

int main()
{
    int N;
    cin >> N;
    vector<int> l;
    for(int i = 0; i < N; ++i) {
	int a;
	cin >> a;
	l.push_back(a);
    }
    sort(a.begin(), a.end());
    int first = l[0];
    int sec = l[1]
    bool poss = false;
    for(auto it = l.begin() + 2; it != l.end(); ++it) {
	if((*it) < first + sec) {
	    poss = true;
	    break;
	} else {
	    first = sec;
	    sec = (*it);
	}
    }
    cout << (poss ? "possible" : "impossible") << endl;
}
