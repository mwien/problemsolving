#include<iostream>
#include<string>

using namespace std;

void conv2coord(char c, char r, int &x1, int &y1) {
    x1 = (c[0]-'A') - (r[0]-1);
    y1 = (8 - (c[0] - 'A')) - (r[0]-1);
}

