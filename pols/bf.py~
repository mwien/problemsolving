G = [[1, 1, 1, 1, 1, 1, 1], [0, 1, 2, 3, 4, 5, 6], [0, 1, 4, 2, 2, 4, 1], [0, 1, 1, 6, 1, 6, 6]]
for i in range(pow(7, 4)):
    num = []
    for j in range(7):
        num.append(i % 7)
        i /= 7
    for j in range(7):
        s = 0
        for k in range(4):
            s += num[j] * G[k][j]
        sys.stdout.write(str(s) + " ")
    sys.stdout.write("\n")
