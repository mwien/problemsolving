#include<iostream>
#include<vector>
#include<string>
#include<algorithm>
#include<utility>

using namespace std;

int main()
{
    int k,p;
    cin >> k >> p;
    vector<string> z;
    for(int i = 0; i < 9; ++i) {
	string ig = "";
	z.push_back(ig + (char) ('1' + i) + (char) ('1' + i));
    }
    int old = 0;
    for(int i = 0; i < 5; ++i) {
	int cur = z.size();
	for(int j = old; j < cur; ++j) {
	    for(int l = 0; l < 9; ++l) {
		string nig = (char) ('1' + l) + z[j] + (char) ('1' + l);
		z.push_back(nig);
	    }
	}
	old = cur;
    }
    vector<long long> zn;
    for(int i = 0; i < z.size(); ++i) {
	zn.push_back(stoll(z[i]));
    }
    sort(zn.begin(), zn.end());
    long long sum = 0;
    for(int i = 0; i < k; ++i) {
	sum = (sum + zn[i]) % p;
    }
    cout << sum << endl;
}
