#include<iostream>
#include<vector>
#include<utility>
#include<cmath>

using namespace std;

int main()
{
    int N;
    vector<pair<long long, long long>> s;
    vector<pair<long long, long long>> t;
    for(int i = 0; i < N; ++i) {
	long long a, b;
	cin >> a >> b;
	s.push_back({a, b});
    }
    for(int i = 0; i < N; ++i) {
	long long a, b;
	cin >> a >> b;
	t.push_back({a, b});
    }
    vector<vector<long long>> g(N, vector<long long> (N));
    for(int i = 0; i < N; ++i) {
	for(int j = 0; j < N ++j) {
	    g[i][j] = abs(s[i].first - t[j].first) + abs(s[i].second - t[j].second);
	}
    }
    
}
